@model PaperMID.BO.DireccionBO
<div class="row">
    <div class="col-md-12">
        <div class="row">
            <div class="col-md-3">
                <div class="form-group">
                    <label>Calle</label>
                    <input class="form-control" type="text" name="CalleDir" value="@Model.CalleDir" />
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label>Cruzamientos</label>
                    <input class="form-control" type="text" name="CruzaDir" value="@Model.CruzaDir" />
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label>Colonia</label>
                    <input class="form-control" type="text" name="ColoniaDir" value="@Model.ColoniaDir" />
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label>Código Postal</label>
                    <input class="form-control" type="text" name="CPDir" value="@Model.CPDir" />
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="row">
            <div class="col-md-4">
                <div class="form-group">
                    <label>Número Interior</label>
                    <input class="form-control" type="text" name="NumInteDir" value="@Model.NumInteDir" />
                </div>
            </div>
            <div class="col-md-4">
                <div class="form-group">
                    <label>Número Exterior</label>
                    <input class="form-control" type="text" name="NumExteDir" value="@Model.NumExteDir" />
                </div>
            </div>
            <div class="col-md-4">
                <div class="form-group">
                    <label>Municipio</label>
                    @*@Html.DropDownList("IdMunicipio1", null, htmlAttributes: new { @class = "form-control" })*@
                </div>
            </div>
        </div>
    </div>
</div>
